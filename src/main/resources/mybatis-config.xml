<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias alias="SectorDto" type="ee.demo.sectorSelector.models.SectorDto"/>
        <typeAlias alias="UserDto" type="ee.demo.sectorSelector.models.UserDto"/>
    </typeAliases>

    <mappers>
        <mapper resource="mapper/SectorMapper.xml"/>
        <mapper resource="mapper/UserMapper.xml"/>
    </mappers>
</configuration>